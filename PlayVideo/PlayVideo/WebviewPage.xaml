    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    Title="Video Play Page">
    <ContentPage.Content>
        <StackLayout
            Margin="20"
            Background="red"
            VerticalOptions="Center">
            <WebView x:Name="webview" />

        </StackLayout>
    </ContentPage.Content>
</ContentPage>